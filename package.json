{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "react-scripts": "0.9.3"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "codacy-coverage": "^2.0.2",
    "firebase": "^4.1.3",
    "react": "^15.4.2",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.4.2",
    "react-router-dom": "next",
    "react-toggle-switch": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "yarn run test -- --coverage"
  },
  "jest": {
    "collectCoverageFrom": ["*.js"],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
